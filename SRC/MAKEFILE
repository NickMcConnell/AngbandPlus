# This is a make file for Vanilla 2.8.3 with and without the Borg,
# for use with GCC and "Archimedes GNU Make 1.02", etc.


OBJS = \
o.birth o.bldg o.cave o.cmd1 o.cmd2 o.cmd3 o.cmd4 o.cmd5 \
o.cmd6 o.dungeon o.files o.generate o.init1 o.init2 o.load1 \
o.load2 o.main o.main-acn o.melee1 o.melee2 o.monster1 o.monster2 \
o.monster3 o.object1 o.object2 o.save o.spells1 o.spells2 o.store \
o.tables o.util o.variable o.wizard1 o.wizard2 o.xtra1 o.xtra2 o.xtra3 \
o.z-form o.z-rand o.z-term o.z-util o.z-virt

LIBS = DeskLib:o.DeskLib

CC = gcc

CCFLAGS = -mstubs -O2 -idirafter Desklib:

# The normal (non-Borg) !RunImage:

^.!RunImage:	$(OBJS) $(LIBS)
		$(CC) $(CCFLAGS) -o ^.!RunImage $(OBJS) $(LIBS)


